@RestController
public class GapExample{

@Autowired
ServiceClass service;

@GetMapping(value="/data", consumes = MediaType.Application/JSON, produces = MediaType.Application/JSON)
public String getName(@param{id}){

service.getData();
}

@Service
public class ServiceClass{
 public String getData(){
	return "Hello GAP";
}

